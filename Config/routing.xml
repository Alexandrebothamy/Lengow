<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="lengow.export" path="/lengow/export" methods="get">
        <default key="_controller">Lengow\Controller\ExportController::lengowExport</default>
    </route>

    <route id="lengow.configure" path="/admin/module/Lengow/configuration" methods="post">
        <default key="_controller">Lengow\Controller\LengowConfigurationController::configure</default>
    </route>
</routes>
